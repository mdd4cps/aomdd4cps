#################################################################
#    Classes
#################################################################

###  https://example.org/mdd4cps/pim#Action
:Action rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :OnDemandAction
                                            :OnIntervalAction
                                          )
                            ] ;
        rdfs:subClassOf :InternalElement ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasRefinementOperator ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :RefinementOperator
                        ] .


###  https://example.org/mdd4cps/pim#CPComponent
:CPComponent rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :containsElement ;
                               owl:someValuesFrom :InternalElement
                             ] .


###  https://example.org/mdd4cps/pim#CPModel
:CPModel rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasComponent ;
                           owl:someValuesFrom :CPComponent
                         ] .


###  https://example.org/mdd4cps/pim#HWResource
:HWResource rdf:type owl:Class ;
            rdfs:subClassOf :Resource ;
            owl:disjointWith :SWResource .


###  https://example.org/mdd4cps/pim#InternalElement
:InternalElement rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:unionOf ( :Action
                                                     :MessageReceiver
                                                     :MessageSender
                                                     :RefinementOperator
                                                     :Resource
                                                   )
                                     ] ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :isContainedIn ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :CPComponent
                                 ] .


###  https://example.org/mdd4cps/pim#MessageReceiver
:MessageReceiver rdf:type owl:Class ;
                 rdfs:subClassOf :InternalElement ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :forwardsTo ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :InternalElement
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasSender ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :MessageSender
                                 ] .


###  https://example.org/mdd4cps/pim#MessageSender
:MessageSender rdf:type owl:Class ;
               rdfs:subClassOf :InternalElement ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :deliversTo ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :MessageReceiver
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :extractsFrom ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :InternalElement
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :interval_in_milliseconds ;
                                 owl:someValuesFrom xsd:integer
                               ] .


###  https://example.org/mdd4cps/pim#OnDemandAction
:OnDemandAction rdf:type owl:Class ;
                rdfs:subClassOf :Action ;
                owl:disjointWith :OnIntervalAction .


###  https://example.org/mdd4cps/pim#OnIntervalAction
:OnIntervalAction rdf:type owl:Class ;
                  rdfs:subClassOf :Action ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :interval_in_milliseconds ;
                                    owl:someValuesFrom xsd:integer
                                  ] .


###  https://example.org/mdd4cps/pim#RefinementOperator
:RefinementOperator rdf:type owl:Class ;
                    rdfs:subClassOf :InternalElement ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasChildAction ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Action
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasRefinementType ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :RefinementOperatorType
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty [ owl:inverseOf :hasParentAction
                                                     ] ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Action
                                    ] .


###  https://example.org/mdd4cps/pim#RefinementOperatorType
:RefinementOperatorType rdf:type owl:Class .


###  https://example.org/mdd4cps/pim#Resource
:Resource rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( :HWResource
                                              :SWResource
                                            )
                              ] ;
          rdfs:subClassOf :InternalElement .


###  https://example.org/mdd4cps/pim#SWResource
:SWResource rdf:type owl:Class ;
            rdfs:subClassOf :Resource .


