    <!-- https://example.org/mdd4cps/pim#Action -->

    <owl:Class rdf:about="https://example.org/mdd4cps/pim#Action">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://example.org/mdd4cps/pim#OnDemandAction"/>
                    <rdf:Description rdf:about="https://example.org/mdd4cps/pim#OnIntervalAction"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://example.org/mdd4cps/pim#InternalElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://example.org/mdd4cps/pim#hasRefinementOperator"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://example.org/mdd4cps/pim#RefinementOperator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


